<%= @restaurant.name %>
<br>
<%= @restaurant.address %>
<br>
<%= @restaurant.phone_number %>
<br>
<%= @restaurant.category %>
<br>
<p><strong>Reviews</strong></p>
<br>
<% @restaurant.reviews.each do |review| %>
  <%= review.rating %>
  <br>
  <%= review.content %>
<% end %>
<br>
<button><%= link_to 'Add Rating', new_restaurant_review_path(@restaurant) %></button>
<br>
<button><%= link_to 'Back', restaurants_path %></button>
